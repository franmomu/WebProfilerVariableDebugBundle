{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block head %}
    {# the panel content #}
    {{ parent() }}
    <style>
        ol.tree li {
            padding-bottom: 0;
        }
    </style>
{% endblock %}

{% block toolbar %}
    {# the web debug toolbar content #}
    {% if collector.countVariables %}
        {% set icon %}
            <img width="25" height="25" alt="Logs" style="border-width: 0; vertical-align: middle; margin-right: 5px;" src="data:image/png;base64,"/>
        {% endset %}
        {% set text %}
            <span>{{ collector.countVariables }}</span>
        {% endset %}
        {% set icon %}
            <a style="text-decoration: none; margin: 0; padding: 0;" href="{{ path('_profiler', { 'token': token, 'panel': name }) }}">{{ icon }}</a>
        {% endset %}
        <span style="white-space:nowrap; color:#2f2f2f; display:inline-block; min-height:24px; border-right:1px solid #cdcdcd; padding:5px 7px 5px 4px; ">
             {{ icon|default('') }}
             {{ text|default('') }}
        </span>
    {% endif %}
{% endblock %}

{% block menu %}
    {# the menu content #}
    <span class="label">
	    <span class="icon"><img width="32" height="32" alt="Debug" style="border-width: 0; vertical-align: middle; margin-right: 5px;" src="data:image/png;base64,"/></span>
	    <strong>Debug</strong>
        <span class="count">
            <span>{{ collector.countVariables }} variables</span>                
        </span>
	</span>
{% endblock %}

{% block panel %}
    {# the panel content #}
    <h2>Variables to debug</h2>
    {% for variable in collector.debug %}
    	{{ variable | ladybug_dump }}
    {% else %}
    	No hay variables
    {% endfor %}
{% endblock %}